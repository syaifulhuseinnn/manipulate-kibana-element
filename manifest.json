{
    "name": "Manipulate Kibana Element",
    "version": "1.0",
    "description": "Extension for fixing trouble with filter input in Kibana 7.6.0",
    "manifest_version": 2,
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "content_scripts": [
        {
            "matches": ["https://*/app/kibana*"],
            "js": ["contentScript.js"],
            "run_at": "document_end"
        }
    ],
    "page_action": {
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
}
